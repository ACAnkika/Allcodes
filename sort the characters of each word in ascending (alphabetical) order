#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    vector<string> words;
    string word;

    // Read words until end of input
    while (cin >> word) {
        words.push_back(word);
    }

    // Sort characters in each word
    for (string &w : words) {
        sort(w.begin(), w.end());
    }

    // Print output with space
    for (int i = 0; i < words.size(); i++) {
        cout << words[i];
        if (i != words.size() - 1) 
            cout << " ";
    }

    return 0;
}
